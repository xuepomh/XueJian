package com.dh.game.vo.base;

import java.util.ArrayList;
import java.util.List;

/**
 * 怪物组
 * 
 * @author dingqu-pc100
 * 
 */
public class BaseMonsterGroupVO {
	List<BaseMonsterVO> monsters = new ArrayList<BaseMonsterVO>(3);
	private int id;
	private int type;
	private int level;
	private String group;
	private int rewardid;
	private int url;
	private int array;
	private String machine;
	private List<Integer> machines = new ArrayList<Integer>(2);
	private int star;// 星级
	private String name;
	private int value;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public List<BaseMonsterVO> getMonsters() {
		return monsters;
	}

	public void setMonsters(List<BaseMonsterVO> monsters) {
		this.monsters = monsters;
	}

	public int getType() {
		return type;
	}

	public void setType(int type) {
		this.type = type;
	}

	public int getLevel() {
		return level;
	}

	public void setLevel(int level) {
		this.level = level;
	}

	public int getUrl() {
		return url;
	}

	public void setUrl(int url) {
		this.url = url;
	}

	public String getGroup() {
		return group;
	}

	public void setGroup(String group) {
		this.group = group;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getRewardid() {
		return rewardid;
	}

	public void setRewardid(int rewardid) {
		this.rewardid = rewardid;
	}

	public int getArray() {
		return array;
	}

	public void setArray(int array) {
		this.array = array;
	}

	public String getMachine() {
		return machine;
	}

	public void setMachine(String machine) {
		this.machine = machine;
	}

	public int getStar() {
		return star;
	}

	public void setStar(int star) {
		this.star = star;
	}

	public List<Integer> getMachines() {
		return machines;
	}

	public void setMachines(List<Integer> machines) {
		this.machines = machines;
	}

	public int getValue() {
		return value;
	}

	public void setValue(int value) {
		this.value = value;
	}

}
