package com.dh.game.vo.base;

import java.util.ArrayList;
import java.util.List;

public class BaseTaskVO {
	private int id; // id
	private String name; // 任务名称
	private int type; // 任务类型
	private int level; // 所需玩家等级
	private int frontmission; // 前置任务
	private int needmoney; // 接取需要金钱
	private int needitem; // 接取需要道具
	private int needhero; // 接取需要英雄
	private String dialogue; // 任务描述
	private String objective; // 任务目标
	private int npc; // npc
	private int completetype; // 任务完成类型
	private int targetid; // 任务对象ID
	private int number; // 任务数目
	private int taskreward; // 奖励组

	private List<BaseTaskVO> nextBaseTaskVO = new ArrayList<BaseTaskVO>();

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getType() {
		return type;
	}

	public void setType(int type) {
		this.type = type;
	}

	public int getLevel() {
		return level;
	}

	public void setLevel(int level) {
		this.level = level;
	}

	public int getFrontmission() {
		return frontmission;
	}

	public void setFrontmission(int frontmission) {
		this.frontmission = frontmission;
	}

	public int getNeedmoney() {
		return needmoney;
	}

	public void setNeedmoney(int needmoney) {
		this.needmoney = needmoney;
	}

	public int getNeeditem() {
		return needitem;
	}

	public void setNeeditem(int needitem) {
		this.needitem = needitem;
	}

	public int getNeedhero() {
		return needhero;
	}

	public void setNeedhero(int needhero) {
		this.needhero = needhero;
	}

	public String getDialogue() {
		return dialogue;
	}

	public void setDialogue(String dialogue) {
		this.dialogue = dialogue;
	}

	public String getObjective() {
		return objective;
	}

	public void setObjective(String objective) {
		this.objective = objective;
	}

	public int getNpc() {
		return npc;
	}

	public void setNpc(int npc) {
		this.npc = npc;
	}

	public int getCompletetype() {
		return completetype;
	}

	public void setCompletetype(int completetype) {
		this.completetype = completetype;
	}

	public int getTargetid() {
		return targetid;
	}

	public void setTargetid(int targetid) {
		this.targetid = targetid;
	}

	public int getNumber() {
		return number;
	}

	public void setNumber(int number) {
		this.number = number;
	}

	public int getTaskreward() {
		return taskreward;
	}

	public void setTaskreward(int taskreward) {
		this.taskreward = taskreward;
	}

	public List<BaseTaskVO> getNextBaseTaskVO() {
		return nextBaseTaskVO;
	}

	public void setNextBaseTaskVO(List<BaseTaskVO> nextBaseTaskVO) {
		this.nextBaseTaskVO = nextBaseTaskVO;
	}

}
