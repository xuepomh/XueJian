package com.dh.game.vo.base;

public class BaseProteVO {
	private int id; // 等级id
	private String name; // 等级名称
	private int protectionExp; // 升级所需护佑值
	private int money; // 单次消耗银两
	private int itemId; // 所需道具id
	private int itemNumber; // 道具数量
	private int itemExp; // 符文护佑增加值
	private int rmb; // 单次消耗元宝
	private int rmbExp; // 元宝增加护佑值
	private int critRate; // 暴击几率
	private int critNumber; // 暴击值
	private int stats; // 属性加成
	private int equipmentStar; // 装备星级限制

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getProtectionExp() {
		return protectionExp;
	}

	public void setProtectionExp(int protectionExp) {
		this.protectionExp = protectionExp;
	}

	public int getMoney() {
		return money;
	}

	public void setMoney(int money) {
		this.money = money;
	}

	public int getItemId() {
		return itemId;
	}

	public void setItemId(int itemId) {
		this.itemId = itemId;
	}

	public int getItemNumber() {
		return itemNumber;
	}

	public void setItemNumber(int itemNumber) {
		this.itemNumber = itemNumber;
	}

	public int getItemExp() {
		return itemExp;
	}

	public void setItemExp(int itemExp) {
		this.itemExp = itemExp;
	}

	public int getRmb() {
		return rmb;
	}

	public void setRmb(int rmb) {
		this.rmb = rmb;
	}

	public int getRmbExp() {
		return rmbExp;
	}

	public void setRmbExp(int rmbExp) {
		this.rmbExp = rmbExp;
	}

	public int getCritRate() {
		return critRate;
	}

	public void setCritRate(int critRate) {
		this.critRate = critRate;
	}

	public int getCritNumber() {
		return critNumber;
	}

	public void setCritNumber(int critNumber) {
		this.critNumber = critNumber;
	}

	public int getStats() {
		return stats;
	}

	public void setStats(int stats) {
		this.stats = stats;
	}

	public int getEquipmentStar() {
		return equipmentStar;
	}

	public void setEquipmentStar(int equipmentStar) {
		this.equipmentStar = equipmentStar;
	}

}
