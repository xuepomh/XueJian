package com.dh.game.vo.base;

public class BaseLegionBossVO {
	private int id;
	private int monster;// 怪物id
	private int gangreward;
	private int energy;
	private int level;// 限制帮派等级
	private BaseMonsterVO baseMonsterVO;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getMonster() {
		return monster;
	}

	public void setMonster(int monster) {
		this.monster = monster;
	}

	public int getGangreward() {
		return gangreward;
	}

	public void setGangreward(int gangreward) {
		this.gangreward = gangreward;
	}

	public int getEnergy() {
		return energy;
	}

	public void setEnergy(int energy) {
		this.energy = energy;
	}

	public int getLevel() {
		return level;
	}

	public void setLevel(int level) {
		this.level = level;
	}

	public BaseMonsterVO getBaseMonsterVO() {
		return baseMonsterVO;
	}

	public void setBaseMonsterVO(BaseMonsterVO baseMonsterVO) {
		this.baseMonsterVO = baseMonsterVO;
	}

}
